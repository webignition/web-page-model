{
    "name": "webignition/web-page-model",
    "description": "Model of a web page, based off webignition/web-resource-model",
    "keywords": ["model", "webpage"],
    "homepage": "https://github.com/webignition/web-page-model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\WebPage\\": "src/",
            "webignition\\Tests\\WebResource\\WebPage\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "webignition/web-resource-model":">=2,<3",
        "querypath/QueryPath": ">=3.0.0",
        "webignition/internet-media-type":">=0.1,<1.0",
        "webignition/character-set-list":">=0.2,<1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~1.0"
    },
    "minimum-stability":"stable",
    "prefer-stable":true
}